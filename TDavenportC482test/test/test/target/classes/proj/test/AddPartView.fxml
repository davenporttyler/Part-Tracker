            <ToggleGroup fx:id="radioToggle" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="APOutsourcedRadio" layoutX="307.0" layoutY="63.0" mnemonicParsing="false" onAction="#radioToggleFunction" text="Outsourced" toggleGroup="$radioToggle">
